{
  "author": "inc2734",
  "license": "GPL-2.0+",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@fortawesome/fontawesome-free": "^5.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.9",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "classnames": "^2.2.6",
    "cpy-cli": "^2.0.0",
    "cssnano-cli": "^1.0.5",
    "eslint": "^5.10.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-wordpress": "git://github.com/WordPress-Coding-Standards/eslint-plugin-wordpress.git#1774343f6226052a46b081e01db3fca8793cc9f1",
    "masonry-layout": "^4.2.2",
    "react-select": "^2.1.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "po2json": "^1.0.0-alpha",
    "postcss-cli": "^6.0.1",
    "rimraf": "^2.6.2",
    "sass-basis": "^9.2.7",
    "slick-carousel": "^1.8.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "clean:packages": "rimraf dist/packages",
    "clean:zip": "rimraf snow-monkey-blocks.zip",
    "build": "npm-run-all -p css js packages po2json",
    "prepublish": "npm run build",
    "lint": "./node_modules/.bin/eslint .",
    "minify:css:front": "cssnano dist/css/blocks.css dist/css/blocks.min.css --no-zindex && rimraf dist/css/blocks.css",
    "minify:css:front-nopro": "cssnano dist/css/blocks-nopro.css dist/css/blocks-nopro.min.css --no-zindex && rimraf dist/css/blocks-nopro.css",
    "minify:css:editor": "cssnano dist/css/blocks-editor.css dist/css/blocks-editor.min.css --no-zindex && rimraf dist/css/blocks-editor.css",
    "minify:css:editor-nopro": "cssnano dist/css/blocks-editor-nopro.css dist/css/blocks-editor-nopro.min.css --no-zindex && rimraf dist/css/blocks-editor-nopro.css",
    "minify:css:editor-wrapper": "cssnano dist/css/blocks-editor-wrapper.css dist/css/blocks-editor-wrapper.min.css --no-zindex && rimraf dist/css/blocks-editor-wrapper.css",
    "minify:css:fallback": "cssnano dist/css/fallback.css dist/css/fallback.min.css --no-zindex && rimraf dist/css/fallback.css",
    "css:front:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks.scss -o dist/css",
    "css:front:postcss": "postcss dist/css/blocks.css -r --no-map -c=postcss.config.js",
    "css:front-nopro:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-nopro.scss -o dist/css",
    "css:front-nopro:postcss": "postcss dist/css/blocks-nopro.css -r --no-map -c=postcss.config.js",
    "css:editor:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-editor.scss -o dist/css",
    "css:editor:postcss": "postcss dist/css/blocks-editor.css -r --no-map -c=postcss.config.js",
    "css:editor-nopro:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-editor-nopro.scss -o dist/css",
    "css:editor-nopro:postcss": "postcss dist/css/blocks-editor-nopro.css -r --no-map -c=postcss.config.js",
    "css:editor-wrapper:sass": "node-sass src/css/blocks-editor-wrapper.scss -o dist/css",
    "css:editor-wrapper:postcss": "postcss dist/css/blocks-editor-wrapper.css -r --no-map -c=postcss.config.js",
    "css:fallback:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/fallback.scss -o dist/css",
    "css:fallback:postcss": "postcss dist/css/fallback.css -r --no-map -c=postcss.config.js",
    "css:front": "npm-run-all -s css:front:sass css:front:postcss && npm run minify:css:front",
    "css:front-nopro": "npm-run-all -s css:front-nopro:sass css:front-nopro:postcss && npm run minify:css:front-nopro",
    "css:editor": "npm-run-all -s css:editor:sass css:editor:postcss && npm run minify:css:editor",
    "css:editor-nopro": "npm-run-all -s css:editor-nopro:sass css:editor-nopro:postcss && npm run minify:css:editor-nopro",
    "css:editor-wrapper": "npm-run-all -s css:editor-wrapper:sass css:editor-wrapper:postcss && npm run minify:css:editor-wrapper",
    "css:fallback": "npm-run-all -s css:fallback:sass css:fallback:postcss && npm run minify:css:fallback",
    "css": "npm run clean:css && npm-run-all -p css:*",
    "js:front": "webpack src/js/app.js -o dist/js/app.min.js --config webpack.config.js",
    "js:editor": "webpack src/js/blocks.js -o dist/js/blocks-build.js --config webpack.config.js",
    "js:editor-nopro": "webpack src/js/blocks-nopro.js -o dist/js/blocks-nopro-build.js --config webpack.config.js",
    "js": "npm run clean:js && npm-run-all -p js:*",
    "packages:fontawesome": "cpy 'fontawesome-free/js/**' 'fontawesome-free/LICENSE.txt' '../../dist/packages' --cwd=node_modules/@fortawesome --parents",
    "packages:slick-carousel": "cpy 'slick/**' '../../dist/packages' --cwd=node_modules/slick-carousel --parents",
    "packages:masonry-layout": "cpy 'dist/**' '../../dist/packages/masonry-layout' --cwd=node_modules/masonry-layout --parents",
    "packages": "npm run clean:packages && npm-run-all packages:*",
    "zip:pack": "rsync -a --exclude='/vendor' --exclude='/node_modules' --exclude='.*' --exclude='*.ruleset.xml' --exclude='*.config.js' --exclude='*.xml.dist' --exclude='/bin' --exclude='/tests' --exclude='package*.json' . snow-monkey-blocks && cd snow-monkey-blocks && composer install --no-dev",
    "zip:archive": "zip -9 -qmr snow-monkey-blocks.zip snow-monkey-blocks",
    "zip": "npm run clean:zip && npm run zip:pack && npm run zip:archive",
    "sync-sm-css": "rimraf src/css/core && cp -r ../../themes/snow-monkey/resources/src/css/core ./src/css/core && rimraf src/css/core/_core.scss",
    "po2json:snow-monkey-blocks-editor": "po2json languages/snow-monkey-blocks-ja.po languages/snow-monkey-blocks-ja-snow-monkey-blocks-editor.json -f jed",
    "po2json": "npm-run-all -p po2json:*"
  },
  "dependencies": {
    "@wordpress/i18n": "^3.1.0"
  }
}
